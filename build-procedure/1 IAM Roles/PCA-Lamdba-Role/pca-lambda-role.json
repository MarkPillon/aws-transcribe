{
    "Version": "2012-10-17",
    "Statement": [
     
      {
        "Effect": "Allow",
        "Action": [
          "s3:GetObject",
          "s3:PutObject",
          "s3:ListBucket"
        ],
        "Resource": [
          "arn:aws:s3:::input-audio-bucket1379/*",
          "arn:aws:s3:::metadata-output-bucket1379/*"
        ]
      },
     
      {
        "Effect": "Allow",
        "Action": "states:StartExecution",
        "Resource": "arn:aws:states:<REGION>:<ACCOUNT_ID>:stateMachine:*"
      },

      {
        "Effect": "Allow",
        "Action": [
          "dynamodb:PutItem",
          "dynamodb:UpdateItem"
        ],
        "Resource": "arn:aws:dynamodb:<REGION>:<ACCOUNT_ID>:table/CallAnalytics"
      },

      {
        "Effect": "Allow",
        "Action": [
          "logs:CreateLogGroup",
          "logs:CreateLogStream",
          "logs:PutLogEvents"
        ],
        "Resource": "*"
      }
    ]
  }